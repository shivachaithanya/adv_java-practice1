<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Servlettosevlet2</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>helloservlet</servlet-name>
  <servlet-class>controller.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>helloservlet</servlet-name>
  <url-pattern>/color</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>colorappender</servlet-name>
  <servlet-class>controller.ColorAppender</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>colorappender</servlet-name>
  <url-pattern>/hello</url-pattern>
  </servlet-mapping>
</web-app>